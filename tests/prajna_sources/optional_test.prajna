@test
func TestMain() {
    var opt = Optional<i64>::Create(42);
    test::Assert(opt.HasValue());
    test::Assert(opt.Value == 42);
    var tmp = opt.Value;
    test::Assert(tmp == 42);


    var none : Optional<i64>;
    test::Assert(!none.HasValue());

    var new : Optional<i64>;
    new.Value = 456;
    test::Assert(new.Value == 456);


}
