
struct People{
    name: String;
    _age: i64;
}

interface ageProperty {
    func Get()->i64;
    func Set(v: i64);
}

implement ageProperty for People{
    func Get()->i64{
        return this._age;
    }

    func Set(v: i64){
        this._age = v;
    }
}

@test
func testProperty(){
    var people = People("Mike", 18);
    debug::Assert(people._age == 18);
    // 我们可以像字段一样使用属性
    debug::Assert(people.age == 18);
    people.age = 22;
    debug::Assert(people._age == 22);
    debug::Assert(people.age == 22);

    people.age;
}
