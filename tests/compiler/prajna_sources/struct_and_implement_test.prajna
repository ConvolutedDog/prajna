
struct People{
    name: String;
    age: i64;
}

implement People{
    func sayHello(){
        "Hello\n".Print();
    }

    func sayHi(){
        this.sayHello();
        ("Hi, My name is " + this.name + ".\n").Print();
    }

    // static注解则为静态成员函数
    @static
    func getAge(people: People)->i64{
        return people.age;
    }
}

struct NestedMemberFunction {}

implement NestedMemberFunction {
    func test() {
        this.test();
    }
}

@test
func testStructAndImplement(){
    var mike = People("Mike", 18);
    // "."索引成员函数
    mike.sayHi();
    // 注意域成员都是通过::来索引的
    debug::Assert(People::getAge(mike) == 18);
}
